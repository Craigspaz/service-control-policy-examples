{
   "Version":"2012-10-17",
   "Statement":[
      {
         "Effect":"Deny",
         "Action":[
            "iam:CreateAccessKey",
            "iam:CreateUser",
            "iam:UpdateLoginProfile",
            "iam:CreateLoginProfile",
            "iam:DeleteLoginProfile",
            "iam:UpdateAccountPasswordPolicy",
            "iam:DeleteAccountPasswordPolicy"
         ],
         "Resource":"*",
         "Condition":{
            "ArnNotLike":{
               "aws:PrincipalARN":"arn:aws:iam::${Account}:role/[PRIVILEGED_ROLE]"
            }
         }
      }
   ]
}